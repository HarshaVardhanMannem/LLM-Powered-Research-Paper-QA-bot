[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    .pytest_cache,
    .mypy_cache,
    build,
    dist,
    *.egg-info,
    .tox,
    .coverage,
    htmlcov,
    .DS_Store,
    node_modules
per-file-ignores =
    __init__.py:F401
